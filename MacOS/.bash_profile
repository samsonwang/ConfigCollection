
# List files in more easy way
alias ll='ls -lh'
alias la='ll -a'

alias rm='rm -i'

# git related
alias g='git status --short -b'
alias ga='git add'
alias gb='git branch'
alias gc='git commit -m'
alias gd='git diff'
alias gdc='git diff --cached'
alias gco='git checkout'
alias gps='git push'
alias gpl='git pull'

export PS1="\u @ \W$ "

